<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Fondo32"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/merkatua_fondo"
    tools:context=".Actividad_32_Precios">

    <ImageView
        android:id="@+id/FondoNaraja"
        android:layout_width="342dp"
        android:layout_height="529dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="13dp"
        android:src="@color/Naranjafondos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47" />

    <ImageView
        android:id="@+id/Indabak"
        android:layout_width="114dp"
        android:layout_height="113dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:src="@mipmap/indabak"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.034" />

    <ImageView
        android:id="@+id/kipula"
        android:layout_width="107dp"
        android:layout_height="101dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:src="@mipmap/kipulamorea"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.304" />

    <ImageView
        android:id="@+id/Txorizo"
        android:layout_width="117dp"
        android:layout_height="128dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="56dp"
        android:src="@mipmap/txorizoa"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/Azenario"
        android:layout_width="119dp"
        android:layout_height="98dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:src="@mipmap/azenarioa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.538" />

    <EditText
        android:id="@+id/KipulapPrezio"
        android:layout_width="103dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/VerdeChungo"
        android:ems="10"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.382" />

    <EditText
        android:id="@+id/AzenarioPrezio"
        android:layout_width="103dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/VerdeChungo"
        android:backgroundTint="@color/VerdeChungo"
        android:ems="10"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.643" />

    <EditText
        android:id="@+id/TxorizoPrezio"
        android:layout_width="103dp"
        android:layout_height="29dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/VerdeChungo"
        android:ems="10"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.81" />

    <EditText
        android:id="@+id/IndabakPrezio"
        android:layout_width="103dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/VerdeChungo"
        android:ems="10"
        android:inputType="number|numberDecimal"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/Total"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/VerdeChungo"
        android:backgroundTint="@color/VerdeChungo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TxorizoPrezio"
        app:layout_constraintVertical_bias="0.733" />

    <ImageView
        android:id="@+id/BottonCamaraPrecios"
        android:layout_width="73dp"
        android:layout_height="63dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/camaraverdepnj"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="161dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="PREZIO /KILOKO"
        android:textColor="@color/Verde"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/FondoNaraja"
        app:layout_constraintEnd_toEndOf="@+id/FondoNaraja"
        app:layout_constraintHorizontal_bias="0.782"
        app:layout_constraintStart_toStartOf="@+id/Azenario"
        app:layout_constraintTop_toTopOf="@+id/FondoNaraja"
        app:layout_constraintVertical_bias="0.03" />

</android.support.constraint.ConstraintLayout>